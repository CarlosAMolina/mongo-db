  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "3.2.5"
  },
  "dependencies": {
    "mongoose": "^8.3.3"
  }
